			<xs:element name="UsesEnumsFromTag" type="Tag_t" minOccurs="0" od:jetType="longinteger" od:sqlSType="int"/>
			<xs:element name="UnionDataType" type="UnionDataType_t" minOccurs="0" od:jetType="text" od:sqlSType="nvarchar"/>
		</xs:sequence>
