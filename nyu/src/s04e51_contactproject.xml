<?xml version="1.0" encoding="UTF-8"?>
<project name="s04e51_contactproject" default="all">
  
  
  <property file="s04e51_contactproject.properties"/>
  <!-- Uncomment the following property if no tests compilation is needed -->
  <!-- 
  <property name="skip.tests" value="true"/>
   -->
  
  <!-- Compiler options -->
  
  <property name="compiler.debug" value="on"/>
  <property name="compiler.generate.no.warnings" value="off"/>
  <property name="compiler.args" value=""/>
  <property name="compiler.max.memory" value="700m"/>
  <patternset id="ignored.files">
    <exclude name="**/CVS/**"/>
    <exclude name="**/SCCS/**"/>
    <exclude name="**/RCS/**"/>
    <exclude name="**/rcs/**"/>
    <exclude name="**/.DS_Store/**"/>
    <exclude name="**/.svn/**"/>
    <exclude name="**/.pyc/**"/>
    <exclude name="**/.pyo/**"/>
    <exclude name="**/*.pyc/**"/>
    <exclude name="**/*.pyo/**"/>
    <exclude name="**/.git/**"/>
    <exclude name="**/*.hprof/**"/>
    <exclude name="**/_svn/**"/>
    <exclude name="**/.hg/**"/>
    <exclude name="**/*.lib/**"/>
    <exclude name="**/*~/**"/>
    <exclude name="**/__pycache__/**"/>
    <exclude name="**/.bundle/**"/>
    <exclude name="**/*.rbc/**"/>
  </patternset>
  <patternset id="library.patterns">
    <include name="*.zip"/>
    <include name="*.apk"/>
    <include name="*.war"/>
    <include name="*.egg"/>
    <include name="*.ear"/>
    <include name="*.ane"/>
    <include name="*.swc"/>
    <include name="*.jar"/>
  </patternset>
  <patternset id="compiler.resources">
    <exclude name="**/?*.java"/>
    <exclude name="**/?*.form"/>
    <exclude name="**/?*.class"/>
    <exclude name="**/?*.groovy"/>
    <exclude name="**/?*.scala"/>
    <exclude name="**/?*.flex"/>
    <exclude name="**/?*.kt"/>
    <exclude name="**/?*.clj"/>
  </patternset>
  
  <!-- JDK definitions -->
  
  <property name="jdk.bin.1.6" value="${jdk.home.1.6}/bin"/>
  <path id="jdk.classpath.1.6">
    <fileset dir="${jdk.home.1.6}">
      <include name="lib/deploy.jar"/>
      <include name="lib/dt.jar"/>
      <include name="lib/javaws.jar"/>
      <include name="lib/jce.jar"/>
      <include name="lib/jconsole.jar"/>
      <include name="lib/management-agent.jar"/>
      <include name="lib/plugin.jar"/>
      <include name="lib/sa-jdi.jar"/>
      <include name="../Classes/charsets.jar"/>
      <include name="../Classes/classes.jar"/>
      <include name="../Classes/jsse.jar"/>
      <include name="../Classes/ui.jar"/>
      <include name="lib/ext/apple_provider.jar"/>
      <include name="lib/ext/dnsns.jar"/>
      <include name="lib/ext/localedata.jar"/>
      <include name="lib/ext/sunjce_provider.jar"/>
      <include name="lib/ext/sunpkcs11.jar"/>
    </fileset>
  </path>
  
  <property name="project.jdk.home" value="${jdk.home.1.6}"/>
  <property name="project.jdk.bin" value="${jdk.bin.1.6}"/>
  <property name="project.jdk.classpath" value="jdk.classpath.1.6"/>
  <!-- Register Custom Compiler Taskdefs -->
  <property name="javac2.home" value="${idea.home}/lib"/>
  <path id="javac2.classpath">
    <pathelement location="${javac2.home}/javac2.jar"/>
    <pathelement location="${javac2.home}/jdom.jar"/>
    <pathelement location="${javac2.home}/asm-all.jar"/>
    <pathelement location="${javac2.home}/jgoodies-forms.jar"/>
  </path>
  <target name="register.custom.compilers">
    <taskdef name="javac2" classname="com.intellij.ant.Javac2" classpathref="javac2.classpath"/>
    <taskdef name="instrumentIdeaExtensions" classname="com.intellij.ant.InstrumentIdeaExtensions" classpathref="javac2.classpath"/>
  </target>
  
  <!-- Modules -->
  
  
  <!-- Module S04E51_ContactProject -->
  
  <dirname property="module.s04e51_contactproject.basedir" file="${ant.file}"/>
  
  
  <property name="module.jdk.home.s04e51_contactproject" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.s04e51_contactproject" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.s04e51_contactproject" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.s04e51_contactproject" value="-encoding UTF-8 -source 1.6 ${compiler.args}"/>
  
  <property name="s04e51_contactproject.output.dir" value="${module.s04e51_contactproject.basedir}/out/production/S04E51_ContactProject"/>
  <property name="s04e51_contactproject.testoutput.dir" value="${module.s04e51_contactproject.basedir}/out/test/S04E51_ContactProject"/>
  
  <path id="s04e51_contactproject.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="s04e51_contactproject.module.production.classpath">
    <path refid="${module.jdk.classpath.s04e51_contactproject}"/>
  </path>
  
  <path id="s04e51_contactproject.runtime.production.module.classpath">
    <pathelement location="${s04e51_contactproject.output.dir}"/>
  </path>
  
  <path id="s04e51_contactproject.module.classpath">
    <path refid="${module.jdk.classpath.s04e51_contactproject}"/>
    <pathelement location="${s04e51_contactproject.output.dir}"/>
  </path>
  
  <path id="s04e51_contactproject.runtime.module.classpath">
    <pathelement location="${s04e51_contactproject.testoutput.dir}"/>
    <pathelement location="${s04e51_contactproject.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.s04e51_contactproject">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.s04e51_contactproject">
    <patternset refid="excluded.from.module.s04e51_contactproject"/>
  </patternset>
  
  <path id="s04e51_contactproject.module.sourcepath">
    <dirset dir="${module.s04e51_contactproject.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.s04e51_contactproject" depends="compile.module.s04e51_contactproject.production,compile.module.s04e51_contactproject.tests" description="Compile module S04E51_ContactProject"/>
  
  <target name="compile.module.s04e51_contactproject.production" depends="register.custom.compilers" description="Compile module S04E51_ContactProject; production classes">
    <mkdir dir="${s04e51_contactproject.output.dir}"/>
    <javac2 destdir="${s04e51_contactproject.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.s04e51_contactproject}/javac">
      <compilerarg line="${compiler.args.s04e51_contactproject}"/>
      <bootclasspath refid="s04e51_contactproject.module.bootclasspath"/>
      <classpath refid="s04e51_contactproject.module.production.classpath"/>
      <src refid="s04e51_contactproject.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.s04e51_contactproject"/>
    </javac2>
    
    <copy todir="${s04e51_contactproject.output.dir}">
      <fileset dir="${module.s04e51_contactproject.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.s04e51_contactproject.tests" depends="register.custom.compilers,compile.module.s04e51_contactproject.production" description="compile module S04E51_ContactProject; test classes" unless="skip.tests"/>
  
  <target name="clean.module.s04e51_contactproject" description="cleanup module">
    <delete dir="${s04e51_contactproject.output.dir}"/>
    <delete dir="${s04e51_contactproject.testoutput.dir}"/>
  </target>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.s04e51_contactproject" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.s04e51_contactproject" description="build all modules"/>
  
  <target name="all" depends="build.modules" description="build all"/>
</project>